<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="Inställningar">

        <EditTextPreference
            app:key="hypothesis"
            app:title="Nollhypotes (H0)"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="significance"
            app:title="Signifikansnivå (α)"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:key="col1"
            app:title="Kolumn 1"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="col2"
            app:title="Kolumn 2"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="row1"
            app:title="Rad 1"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:useSimpleSummaryProvider="true" />

        <EditTextPreference
            app:key="row2"
            app:title="Rad 2"
            android:selectAllOnFocus="true"
            android:singleLine="true"
            app:useSimpleSummaryProvider="true" />

    </PreferenceCategory>

</PreferenceScreen>